<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RunPostBuildEvent></RunPostBuildEvent>
    <Platforms>AnyCPU;x64;x86</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="generated\" />
  </ItemGroup>

  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <Exec Command="echo del /Q $(ProjectDir)generated\*&#xD;&#xA;del /Q $(ProjectDir)generated\*&#xD;&#xA;&#xD;&#xA;echo copy $(ProjectDir)..\ompl_wrap\generated\* $(ProjectDir)\generated\&#xD;&#xA;copy $(ProjectDir)..\ompl_wrap\generated\* $(ProjectDir)\generated\&#xD;&#xA;&#xD;&#xA;echo copy $(ProjectDir)..\ompl_wrap\bin\$(Platform)\$(Configuration)\ompl_wrap.dll $(OutDir)&#xD;&#xA;copy $(ProjectDir)..\ompl_wrap\bin\$(Platform)\$(Configuration)\ompl_wrap.dll $(OutDir)" />
  </Target>

</Project>
